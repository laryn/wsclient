define('WSCLIENT_UI_PATH', 'admin/config/services/wsclient');

/**
 * Implementation of hook_menu()
 */
function wsclient_ui_menu() {
  $items = array();
  $items[WSCLIENT_UI_PATH] = array(
    'title' => 'Web service client',
    'position' => 'right',
    'page callback' => 'drupal_get_form',
